{
    "parser": "babel-eslint",
    "extends": ["airbnb-base"],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "env": {
        "browser": true,
        "node": true
    },
    "rules": {
        "indent": ["warn", 4],
        "quotes": 0,
        "no-underscore-dangle": 0,
        "camelcase": 0,
        "arrow-parens": ["error", "always"],
        "max-len": 0,
        "no-useless-escape": 0,
    }
}
